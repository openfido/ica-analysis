module tape
{
	csv_header_type NAME;
}

object recorder 
{
	parent "Reg1";
	file "${OUTPUT:-.}/regulator.csv";
	interval -1;
	property "tap_A,tap_B,tap_C,power_in_A.real,power_in_B.real,power_in_C.real,power_in.real";
}

object recorder
